    "babelify": "^6.4.0",
    "lodash": "^2.4.1",
    "mocha": "^1.18.2",
    "mocha-stylus-compiler": "^1.0.0",
    "nanodom": "0.0.3",
    "react-addons-test-utils": "^0.14.1",
    "stylusify": "^1.0.0",
    "testdom": "^2.0.0"
  },
